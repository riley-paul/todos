{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "noUnusedLocals": true,
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
}