{
  "name": "astro-test",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm-run-all --parallel dev:db dev:server",
    "build": "astro check && astro build --remote",
    "preview": "astro build && wrangler pages dev --port 4321",
    "test": "vitest --no-file-parallelism",
    "dev:db": "turso dev --db-file dev.db",
    "dev:server": "astro dev --host",
    "cf-typegen": "wrangler types",
    "db:push": "drizzle-kit push",
    "db:push:prod": "NODE_ENV=production drizzle-kit push",
    "db:push:test": "NODE_ENV=test drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:studio:prod": "NODE_ENV=production drizzle-kit studio",
    "db:studio:test": "NODE_ENV=test drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "db:seed:test": "NODE_ENV=test tsx src/db/seed.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/cloudflare": "^12.6.10",
    "@astrojs/react": "^4.4.0",
    "@faker-js/faker": "^9.6.0",
    "@hookform/resolvers": "^5.1.1",
    "@libsql/client": "^0.14.0",
    "@node-rs/argon2": "^1.8.3",
    "@radix-ui/themes": "^3.1.6",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/react-query": "^5.35.5",
    "@tanstack/react-router": "^1.97.25",
    "@tanstack/react-router-devtools": "^1.133.32",
    "@tanstack/router-plugin": "^1.114.17",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "ably": "^2.10.1",
    "arctic": "^3.5.0",
    "astro": "^5.15.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.7.1",
    "jotai": "^2.12.5",
    "lucide-react": "^0.525.0",
    "radix-themes-tw": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^4.1.13",
    "tiny-invariant": "^1.3.3",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.4.5",
    "usehooks-ts": "^3.1.0",
    "vite-plugin-pwa": "^0.20.5",
    "vitest": "^3.0.8",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tanstack/router-devtools": "^1.97.25",
    "@types/node": "^20.12.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "wrangler": "^4.25.0"
  },
  "overrides": {
    "vite": "6.0.2"
  }
}
