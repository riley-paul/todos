{
  "name": "astro-test",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm-run-all --parallel dev:db dev:server",
    "build": "astro check && astro build --remote",
    "preview": "astro build && wrangler pages dev --port 4321",
    "test": "vitest",
    "dev:db": "turso dev --db-file dev.db",
    "dev:server": "astro dev --host",
    "cf-typegen": "wrangler types",
    "db:push": "drizzle-kit push",
    "db:push:prod": "NODE_ENV=production drizzle-kit push",
    "db:push:test": "NODE_ENV=test drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:studio:prod": "NODE_ENV=production drizzle-kit studio",
    "db:studio:test": "NODE_ENV=test drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "db:seed:test": "NODE_ENV=test tsx src/db/seed.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.2.4",
    "@astrojs/react": "^4.2.1",
    "@astrojs/tailwind": "^6.0.0",
    "@faker-js/faker": "^9.6.0",
    "@fontsource-variable/inter": "^5.0.19",
    "@fontsource-variable/rubik": "^5.1.0",
    "@fontsource/fira-sans": "^5.0.20",
    "@hookform/resolvers": "^3.10.0",
    "@libsql/client": "^0.14.0",
    "@node-rs/argon2": "^1.8.3",
    "@radix-ui/themes": "^3.1.6",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.35.5",
    "@tanstack/react-router": "^1.97.25",
    "@tanstack/router-plugin": "^1.114.17",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "arctic": "^3.5.0",
    "astro": "^5.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.7.0",
    "jotai": "^2.12.5",
    "postcss-import": "^16.1.0",
    "radix-themes-tw": "^0.2.3",
    "react": "^18.3.1",
    "react-cookie": "^7.1.4",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tiny-invariant": "^1.3.3",
    "typescript": "^5.4.5",
    "usehooks-ts": "^3.1.0",
    "vaul": "^1.0.0",
    "vite-plugin-pwa": "^0.20.5",
    "vitest": "^3.0.8",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250312.0",
    "@tanstack/router-devtools": "^1.97.25",
    "@types/node": "^20.12.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "wrangler": "^3.114.1"
  },
